APP_NAME="Time Tracker"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_TIMEZONE=UTC
APP_URL=https://yourdomain.com

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

DB_CONNECTION=mysql
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=your_database_name
DB_USERNAME=your_database_user
DB_PASSWORD=your_database_password

SESSION_DRIVER=database
SESSION_LIFETIME=1440
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=lax
SESSION_HTTP_ONLY=true

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# ClickUp Integration
CLICKUP_API_TOKEN=your_clickup_api_token
CLICKUP_SIGNING_SECRET=your_clickup_signing_secret
CLICKUP_TEAM_ID=your_clickup_team_id
CLICKUP_ALLOW_UNVERIFIED=false
# Optional: timezone label for formatting strings in comments/descriptions
# Example: Asia/Manila, America/New_York. Defaults to APP_TIMEZONE.
CLICKUP_DISPLAY_TZ=Asia/Manila

# Time tracking integration flags
# When false, native ClickUp time entries are NOT created. The app will update
# task custom fields and add structured comments for dashboards instead.
CLICKUP_PUSH_TIME_ENTRIES=false

# Reporting list (Integration Table). The app will create a task per time entry
# in this list. Lock this list's permissions in ClickUp so regular users cannot
# edit rows. Provide optional custom field IDs if you want structured data.
CLICKUP_REPORT_LIST_ID=901517284372
# Reporting list custom fields (set only those you created)
# Field Name -> Env Key mapping
# Task ID (Text)
CLICKUP_REPORT_CF_TASK_ID=64bdfab5-a883-4a9e-9068-8e32cc654dd8
# User (Text or Dropdown - if dropdown, integration will write the text label)
CLICKUP_REPORT_CF_USER=51003b69-2e45-45d0-9219-af377d3837fc
# Time In (Date/Time)
CLICKUP_REPORT_CF_TIME_IN=9f64ae36-7b36-4b36-bdda-aa6f65faac83
# Time Out (Date/Time)
CLICKUP_REPORT_CF_TIME_OUT=9d6c2d8c-3747-4fda-936e-73775f91fc1c
# Total Time (mins) (Number)
CLICKUP_REPORT_CF_TOTAL_MINS=a47c4a77-a465-448f-986a-179a013d4471
# Notes (Text area)
CLICKUP_REPORT_CF_NOTES=b8bfcbb4-5c60-454c-92a0-b317430b161f